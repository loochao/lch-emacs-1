\mode<presentation>

\definecolor{tigergold}{RGB}{255,195,0}
\definecolor{darkgold}{RGB}{235,185,40}

\setbeamercolor{structure}{fg=black}
\setbeamercolor{normal text}{fg=black,bg=black!7!white}
\setbeamercolor{alerted text}{parent=normal text,fg=red!70!black}
\setbeamercolor{example text}{parent=normal text,fg=tigergold}

\setbeamercolor{palette primary}{fg=black,bg=tigergold!70}
\setbeamercolor{palette secondary}{fg=black,bg=tigergold!85}
\setbeamercolor{palette tertiary}{fg=black,bg=tigergold}
\setbeamercolor{palette quaternary}{fg=black,bg=tigergold}

\setbeamercolor{titlelike}{parent=palette quaternary}

\setbeamercolor{block title}{fg=black,bg=tigergold}
\setbeamercolor{block title alerted}{use=alerted text,fg=black,bg=alerted text.fg!75!bg}
\setbeamercolor{block title example}{use=example text,fg=black,bg=example text.fg!75!bg}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!25!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!25!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!25!bg}

\setbeamercolor{sidebar}{bg=tigergold!70}

\setbeamercolor{palette sidebar primary}{fg=black}
\setbeamercolor{palette sidebar secondary}{fg=black!75}
\setbeamercolor{palette sidebar tertiary}{fg=black!75}
\setbeamercolor{palette sidebar quaternary}{fg=black}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode
<all>
